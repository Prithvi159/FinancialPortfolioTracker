  },
  "charts": {
    "pie": {
      "title": "Asset Allocation",
      "colors": [
        "#FF6384",
        "#36A2EB",
        "#FFCE56",
        "#4BC0C0",
        "#9966FF",
        "#FF9F40"
      ]
